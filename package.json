{
  "name": "sig-fig",
  "version": "1.0.0",
  "description": "Dev Test for SigFig",
  "main": "app.js",
  "scripts": {
    "test": "./node_modules/.bin/jasmine",
    "build-css": "node-sass sass/main.scss static/css/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build-js": "./node_modules/.bin/browserify client/*.js -o static/js/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "watch-js": "./node_modules/.bin/watchify client/*.js -t [ babelify --presets [ es2015 ] ] -o static/js/bundle.js",
    "start": "node app.js",
    "start:dev": "nodemon app.js"
  },
  "author": "Kevin Truby",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "angular-touch": "^1.6.4",
    "angular-ui-bootstrap": "^2.5.0",
    "body-parser": "^1.17.2",
    "bootstrap-sass": "^3.3.7",
    "express": "^4.15.3",
    "jsonfile": "^3.0.0",
    "lodash": "^4.17.4",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.4",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.6.2",
    "jasmine-node": "^1.14.5",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-read-json": "^1.1.0",
    "karma-safari-launcher": "^1.0.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "watchify": "^3.9.0"
  }
